{
  "expo": {
    "name": "ASD Screener",
    "slug": "asd-screener",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "runtimeVersion": {
      "policy": "sdkVersion"
    },
    "scheme": "asd-screener",
    "ios": {
      "bundleIdentifier": "com.asdscreener.app",
      "supportsTablet": true
    },
    "android": {
      "package": "com.asd_screener.app",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": ["expo-router"],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "3924c0af-4926-4441-891d-5dc9f0d3235c"
      }
    },
    "updates": {
      "url": "https://u.expo.dev/3924c0af-4926-4441-891d-5dc9f0d3235c"
    }
  }
}
